actor: Tester
paths:
    tests: tests
    log: tests/_output
    data: tests/_data
    helpers: tests/_support
settings:
    bootstrap: _bootstrap.php
    colors: true
    memory_limit: 1024M
modules:
    config:
        Db:
            dsn: ''
            user: ''
            password: ''
            dump: tests/_data/dump.sql
env:
    development:
        modules:
            config:
                Db:
                    dsn: 'pgsql:host=localhost;dbname='
                    user: ''
                    password: ''
                    dump: tests/_data/dump.sql
                    cleanup: false
                    populate: false